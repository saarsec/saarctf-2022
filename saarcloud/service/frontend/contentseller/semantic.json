{
  "base": "semantic/",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "dist/",
      "uncompressed": "dist/components/",
      "compressed": "dist/components/",
      "themes": "dist/themes/"
    },
    "clean": "dist/"
  },
  "permission": false,
  "autoInstall": false,
  "rtl": false,
  "components": [
    "reset",
    "site",
    "button",
    "container",
    "header",
    "input",
    "label",
    "loader",
    "segment",
    "form",
    "grid",
    "text",
    "visibility",
    "message",
    "list",
    "checkbox",
    "divider",
    "comment"
  ],
  "version": "2.8.8"
}
